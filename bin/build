FILE=$DOCKER_HOME/images/$1.mk
echo $FILE
if [[ ! -e $FILE ]]; then
    DELETE=true
    FILE="$(mktemp)"
    echo 123
    cat <<- EOF > $FILE
        include \$(DOCKER_HOME)/build/Makefile

        NAME = $1
        VERSION = latest

        build: $1
EOF
fi
make -f $FILE build
if [ ! -z ${DELETE+x} ]; then    
    rm $FILE
fi